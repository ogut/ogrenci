<h1>Listing kullanicis</h1>

<table>
  <tr>
    <th>Ad</th>
    <th>Soyad</th>
    <th>Email</th>
    <th>Adres</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @kullanicis.each do |kullanici| %>
  <tr>
    <td><%= kullanici.ad %></td>
    <td><%= kullanici.soyad %></td>
    <td><%= kullanici.email %></td>
    <td><%= kullanici.adres %></td>
    <td><%= link_to 'Show', kullanici %></td>
    <td><%= link_to 'Edit', edit_kullanici_path(kullanici) %></td>
    <td><%= link_to 'Destroy', kullanici, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

